<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="windowNoTitle">true</item>

    </style>


    <style name="FullScreenTheme" parent="AppTheme">
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowActionBar">false</item>
    </style>


    <!--全透明主题-->
    <style name="TranslucentNavigation" parent="AppTheme">

    </style>
    <!--带有标题栏的style-->
    <style name="TitleStyle">
        <item name="android:fitsSystemWindows">true</item>
        <item name="android:background">@color/colorMain</item>
    </style>
    <!--登录注册APPName-->
    <style name="AppName">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_below">@id/appIcon</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:text">疫苗圈</item>
        <item name="android:textColor">@color/appName</item>
        <item name="android:textSize">23sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="Input">
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@null</item>
        <item name="android:textSize">12sp</item>
        <item name="android:lines">1</item>

    </style>
    <!--登录-输入框字体颜色和大小-->
    <style name="Input.Login" parent="Input">
        <item name="android:textColor">@color/colorLoginInput</item>
        <item name="android:textColorHint">@color/colorLoginInput</item>
    </style>
    <!--注册-输入框字体颜色和大小-->
    <style name="Input.Register" parent="Input">
        <item name="android:textColor">@color/color999</item>
        <item name="android:textColorHint">@color/color999</item>
    </style>
    <!--输入框属性-->
    <style name="InputAttr">
        <item name="android:layout_width">276dp</item>
        <item name="android:layout_height">43dp</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:orientation">horizontal</item>
    </style>
    <!--登录输入框属性-->
    <style name="InputAttr.Login" parent="InputAttr">
        <item name="android:background">@drawable/shape_input_login</item>
    </style>
    <!--注册输入框属性-->
    <style name="InputAttr.Register" parent="InputAttr">
        <item name="android:background">@drawable/shape_input_register</item>
    </style>

    <style name="list_view">
        <item name="android:listSelector">?android:attr/selectableItemBackground</item>
        <item name="android:divider">@color/colorLoginInput</item>
        <item name="android:dividerHeight">0.5dp</item>
    </style>

